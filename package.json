{
  "name": "firecrawl-support-agent",
  "version": "1.0.1",
  "description": "Discord + GitHub + Supabase support bot",
  "type": "module",
  "main": "discord_bot.mjs",
  "scripts": {
    "deploy": "node deploy-commands.mjs",
    "start": "node discord_bot.mjs",
    "dev": "nodemon discord_bot.mjs",

    "seed:labels": "node scripts/seed-labels.mjs",
    "protect:main:ps": "powershell -ExecutionPolicy Bypass -File scripts/protect-main.ps1",
    "protect:main:sh": "bash scripts/protect-main.sh",
    "setup:webhook": "node setup-webhook.mjs",
    "release:patch": "npm version patch -m \"chore(release): %s\" && git push --follow-tags",
    "release:minor": "npm version minor -m \"chore(release): %s\" && git push --follow-tags",
    "release:major": "npm version major -m \"chore(release): %s\" && git push --follow-tags"
  },
  "keywords": [
    "discord",
    "bot",
    "support",
    "supabase",
    "openai"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@azure/msal-node": "^2.6.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@octokit/rest": "^21.0.0",
    "@pinecone-database/pinecone": "^2.0.1",
    "@supabase/supabase-js": "^2.55.0",
    "body-parser": "^1.20.2",
    "discord.js": "^14.16.3",
    "dotenv": "^16.6.1",
    "express": "^4.21.1",
    "node-fetch": "^3.3.2",
    "openai": "^5.13.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
